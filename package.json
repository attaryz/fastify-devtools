{
  "name": "@attaryz/fastify-devtools",
  "version": "0.4.0",
  "description": "Fastify DevTools plugin: dashboard, entry view, metrics, replay, WebSocket capture, Redis tracking, and storage",
  "keywords": [
    "fastify",
    "devtools",
    "development",
    "debugging",
    "dashboard",
    "plugin",
    "monitoring",
    "metrics",
    "websocket",
    "redis",
    "cache"
  ],
  "license": "MIT",
  "author": "Abdullah Ali",
  "homepage": "https://github.com/attaryz/fastify-devtools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/attaryz/fastify-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/attaryz/fastify-devtools/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.js",
      "default": "./dist/types.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json && node ./scripts/copy-views.js",
    "clean": "rimraf dist",
    "test": "tap test/**/*.test.ts",
    "test:watch": "tap test/**/*.test.ts --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "publish": "npm publish --access public"
  },
  "peerDependencies": {
    "fastify": ">=5.0.0",
    "fastify-plugin": ">=5.0.0",
    "mongoose": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "mongoose": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/tap": "^15.0.11",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "mongoose": "^8.0.0",
    "rimraf": "^5.0.0",
    "tap": "^18.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.4.0"
  }
}
