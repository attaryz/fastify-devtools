{
  "name": "@attaryz/fastify-devtools",
  "version": "0.6.3",
  "description": "Fastify DevTools plugin: dashboard, entry view, metrics, replay, WebSocket capture, Redis tracking, and storage",
  "keywords": [
    "fastify",
    "devtools",
    "development",
    "debugging",
    "dashboard",
    "plugin",
    "monitoring",
    "metrics",
    "websocket",
    "redis",
    "cache"
  ],
  "license": "MIT",
  "author": "Abdullah Ali",
  "homepage": "https://github.com/attaryz/fastify-devtools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/attaryz/fastify-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/attaryz/fastify-devtools/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.js",
      "default": "./dist/types.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "packageManager": "yarn@4.3.1",
  "scripts": {
    "build": "tsc -p tsconfig.json && node ./scripts/copy-views.js",
    "clean": "rimraf dist",
    "test": "tap -r ts-node/register test/**/*.test.ts",
    "test:watch": "tap -r ts-node/register test/**/*.test.ts --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "publish": "npm publish --access public",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "biome": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:fix:unsafe": "biome check --write --unsafe .",
    "biome:ci": "biome ci",
    "prepare": "husky"
  },
  "peerDependencies": {
    "fastify": ">=5.0.0",
    "fastify-plugin": ">=5.0.0",
    "mongoose": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "mongoose": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@fastify/websocket": "^11.2.0",
    "@types/node": "^24.7.0",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^16.2.3",
    "mongoose": "^8.0.0",
    "rimraf": "^6.0.1",
    "tap": "^21.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,json}": "biome check --write"
  }
}
